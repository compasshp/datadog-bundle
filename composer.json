{
  "name": "compasshp/datadog-bundle",
  "type": "symfony-bundle",
  "description": "Automatically sends user information to Datadog APM/ASM.",
  "keywords": [],
  "homepage": "https://github.com/compasshp/datadog-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Yudin",
      "email": "mike@compasshp.org"
    }
  ],
  "require": {
    "php": ">=8.0",
    "symfony/config": "~5.3|^6.0",
    "symfony/dependency-injection": "~5.3|^6.0",
    "symfony/event-dispatcher": "~5.3|^6.0",
    "symfony/http-foundation": "~5.3|^6.0",
    "symfony/http-kernel": "~5.3|^6.0",
    "symfony/security-core": "~5.3|^6.0",
    "symfony/security-http": "~5.3|^6.0",
    "symfony/validator": "~5.3|^6.0",
    "symfony/yaml": "~5.3|^6.0",
    "symfony/property-access": "~5.3|^6.0"
  },
  "require-dev": {
    "symfony/framework-bundle": "^5.4|^6.0",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Compass\\DatadogBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "extra": {
    "symfony": {
      "require": "~5.3|^6.0"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true
    },
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
